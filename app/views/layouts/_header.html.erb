<!DOCTYPE html>
<html>
<head>
  <title>Coolapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= favicon_link_tag "images/ico/favicon.png", rel:"shortcut icon" %>
  <%= favicon_link_tag "images/ico/favicon144.png", rel:"shortcut icon", sizes:"144x144" %>
  <%= favicon_link_tag "images/ico/favicon114.png", rel:"shortcut icon", sizes:"114x114" %>  
  <%= favicon_link_tag "images/ico/favicon72.png", rel:"shortcut icon", sizes:"72x72" %>
   <%= favicon_link_tag "images/ico/favicon57.png", rel:"shortcut icon", sizes:"57x57" %>
  <%= csrf_meta_tags %>
</head>
<body data-spy="scroll" data-target="#navbar" data-offset="0" onload="init()">
    <header id="header" role="banner">
        <div class="container">
            <div id="navbar" class="navbar navbar-default">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href=""></a><%=image_tag 'images/coolplum.png', class:'navbar-image' %>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="#main-slider"><i class="icon-home"></i></a></li>
                        <li><a href="#services">Services</a></li>
                        <li><a href="#portfolio">Portfolio</a></li>
                        <!-- <li><a href="#pricing">Pricing</a></li> -->
                        <li><a href="#about-us">About Us</a></li>
                        <li><a href="#contact">Contact</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </header><!--/#header-->